{
    "name": "react-webpack",
    "version": "1.0.0",
    "description": "Create package.json file and install React, Redux, React-Redux, React-Router, Jest. Install and configure Webpack & Babel to get build artifact by running npm command.",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --node-env development --config webpack-configs/webpack.dev.js",
        "build": "webpack --node-env production --config webpack-configs/webpack.prod.js",
        "test": "jest src",
        "test:live": "jest src --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/preset-env": "^7.12.13",
        "@babel/preset-react": "^7.12.13",
        "babel-loader": "^8.2.2",
        "babel-plugin-styled-components": "^1.12.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^5.0.2",
        "html-webpack-plugin": "^5.0.0",
        "jest": "^26.6.3",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "style-loader": "^2.0.0",
        "webpack": "^5.21.2",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2",
        "webpack-merge": "^5.7.3"
    },
    "dependencies": {
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "redux": "^4.0.5",
        "styled-components": "^5.2.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/VladimirOtroshchenko/react-webpack.git"
    },
    "author": "Uladzimir Atroshchanka",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/VladimirOtroshchenko/react-webpack/issues"
    }
}
